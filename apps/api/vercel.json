{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/api/index.js"
    }
  ],
  "includeFiles": [
    "dist/**",
    "api/**",
    "node_modules/**",
    "package.json"
  ]
}
